{
  "name": "altesia_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy:templates": "cpx dist/modules/mail/templates/**/* dist/src/modules/mail/templates && rimraf dist/modules",
    "prebuild": "rimraf dist",
    "postbuild": "npm run copy:templates",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "db:clear": "ts-node -r tsconfig-paths/register database/clear-db.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@liaoliaots/nestjs-redis": "^5.1.0",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.1.2",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "@types/nodemailer": "^6.4.4",
    "bcrypt": "^5.0.1",
    "bull": "^4.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cpx": "^1.5.0",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "express-list-routes": "^1.1.3",
    "ioredis": "^4.28.0",
    "mysql": "^2.18.1",
    "nestjs-config": "^1.4.9",
    "nestjs-typeorm-paginate": "^3.1.3",
    "nodemailer": "^6.7.0",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38",
    "typeorm-seeding": "^1.6.1",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.5",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/ioredis": "^4.28.1",
    "@types/jest": "^26.0.24",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.32",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-unused-imports": "^1.1.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
